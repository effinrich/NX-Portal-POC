{
  "root": "libs/portal/features",
  "sourceRoot": "libs/portal/features/src",
  "projectType": "library",
  "tags": ["scope:portal", "type:feature"],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/portal/features/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/portal/features"],
      "options": {
        "jestConfig": "libs/portal/features/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
