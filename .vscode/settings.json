{
  "workbench.colorCustomizations": {
    "statusBar.noFolderBackground": "#222225",
    "statusBar.debuggingBackground": "#511f1f",
    "editor.lineHighlightBackground": "#1073cf2d",
    "editorBracketMatch.border": "#575B5F",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  "files.exclude": {},
  "explorerExclude.backup": {},
  "vs-kubernetes": {
    "vs-kubernetes.minikube-path": "/Users/$USER/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "terminal.integrated.env.osx": {
    "HELM_EXPERIMENTAL_OCI": "1",
    "SKAFFOLD_NAMESPACE": "rtillman",
    "DOCKER_CONFIG": "~/.docker",
    "HELM_REGISTRY_CONFIG": "$DOCKER_CONFIG/config.json"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.debug": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "prettier.arrowParens": "avoid",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.typescriptEnable": ["typescript", "typescriptreact"],
  "prettier.javascriptEnable": [
    "javascript",
    "javascriptreact"
  ],
  "jestrunner.enableYarnPnpSupport": true
}
